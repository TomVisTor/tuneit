<div class="container">	
	<h2>Mes instruments</h2>

	<%= link_to new_owner_instrument_path  do %>
		<div class="button mb-3">Ajouter un instrument</div>
	<% end %> 

	<div class="row">
		<% @instruments.each do |instrument| %>
		    <div class="col-sm-12 col-md-4">
		        <div class="card">
		          <%= cl_image_tag instrument.photo.key %>
		          <h3><%= instrument.name %></h3>
		        </div>
		    </div>
		<% end %>
	</div>	 
</div>